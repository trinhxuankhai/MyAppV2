<MyCheckBox>:
    cols: 4
    size: root.width, root.height
    #:set fontName "resources/Roboto-Medium"
    box1 : box1
    box2 : box2
    box3 : box3
    box4 : box4
    box5 : box5
    box6 : box6
    box7 : box7
    box8 : box8
    box9 : box9
    box10 : box10
    MDLabel:
        text: "District 1"
        halign: "center"
        font_name: fontName
    MDCheckbox:
        id: box1
        size_hint: None, None
        size: "20dp", "20dp"
        pos_hint: {'center_x': .5, 'center_y': .5}
        on_active: app.on_checkbox_active(box1, '1')
